    "keywords": [
        "adblock",
        "blocklist",
        "content-blocking",
        "filter-parser",
        "filter-rules",
        "igor dimitrijević",
        "igorskyflyer",
        "javascript",
        "lightweight",
        "list-maintainer",
        "npm-module",
        "privacy-tools",
        "rule-counter",
        "typescript",
        "ublock-origin"
    ],
    "license": "MIT",
    "author": "Igor Dimitrijević <support@igorskyflyer.me> (https://github.com/igorskyflyer)",
    "funding": "https://ko-fi.com/igorskyflyer",
    "homepage": "https://github.com/igorskyflyer/npm-adblock-filter-counter",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorskyflyer/npm-adblock-filter-counter.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/npm-adblock-filter-counter/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.5",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.17.0"
